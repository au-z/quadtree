{
  "name": "tsc-noob",
  "version": "1.0.0",
  "description": "",
  "main": "package.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack-dev-server --open --hot --mode=development",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose --env node"
  },
  "author": "Austin Martin (auz.martin@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^23.3.8"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "css-loader": "^1.0.1",
    "style-loader": "0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^22.4.3",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.5",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
